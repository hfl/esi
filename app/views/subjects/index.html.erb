<p id="notice"><%= notice %></p>

<table class="table table-hover">
  <thead>
    <tr>
      <th>主题</th>
      <th>期刊种数</th>
      <th>影响因子总值</th>
      <th>影响因子均值</th>
    </tr>
  </thead>

  <tbody>
    <% @subjects.each do |subject| %>
      <tr>
        <td><%= link_to subject.title, subject %></td>
        <td><%= subject.journal_size %></td>
        <td><%= subject.sum_impact %></td>
        <td><%= subject.av_impact %></td>
      </tr>
    <% end %>
  </tbody>
</table>
